[user]
  name = Yevgeniy Kuksenko
  email = ykuksenko@gmail.com
[alias]
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  stat = status --short --branch
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
[core]
  editor = vim
[push]
  default = simple
[merge]
  tool = vimdiff
